<?xml version='1.0'?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:bf="http://id.loc.gov/ontologies/bibframe/" xmlns:bflc="http://id.loc.gov/ontologies/bflc/" xmlns:madsrdf="http://www.loc.gov/mads/rdf/v1#">
  <bf:Instance>
    <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Electronic"/>
    <bf:hasItem>
      <bf:Item>
        <bf:electronicLocator rdf:resource="http://something.example.org"/>
      </bf:Item>
    </bf:hasItem>
    <bf:hasItem>
      <bf:Item>
        <bf:electronicLocator>
          <rdfs:Resource>
            <bflc:locator rdf:resource="http://something.else.example.org"/>
            <bf:note>
              <bf:Note>
                <rdfs:label>Souped up version</rdfs:label>
              </bf:Note>
            </bf:note>
          </rdfs:Resource>
        </bf:electronicLocator>
      </bf:Item>
    </bf:hasItem>
    <bf:supplementaryContent rdf:resource="http://something.else.again.example.org"/>
  </bf:Instance>
</rdf:RDF>
